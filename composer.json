{
    "name": "repat/laravel-mysql-enum-values",
    "description": "Small Laravel Query Builder Macro that returns valid values for a MySQL ENUM",
    "version": "1.0",
    "keywords": [
        "repat",
        "laravel",
        "mysql",
        "enum",
        "macro",
        "values",
        "valid"
    ],
    "homepage": "https://github.com/repat/laravel-mysql-enum-values",
    "license": "MIT",
    "authors": [
        {
            "name": "repat",
            "email": "repat@repat.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^9.0",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Repat\\LaravelMySqlEnumValues\\": "src"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": false,
            "pestphp/pest-plugin": false
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Repat\\LaravelMySqlEnumValues\\QueryBuilderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
